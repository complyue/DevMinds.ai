{
  "name": "@devminds/webapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json && vite build",
    "dev": "vite",
    "format": "sh -c 'set -e; for f in src/**/*.json; do [ -f \"$f\" ] && pnpm exec sort-json \"$f\" --deep --ignore-case --order asc || true; done' && pnpm exec prettier -w \"src/**/*.{ts,tsx,js,jsx,json,yaml,yml,css,scss,md,mdx,html,htm}\" || true",
    "lint": "echo \"(add linter later)\"",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "6.27.0"
  },
  "devDependencies": {
    "@types/node": "22.7.7",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.2",
    "typescript": "5.6.3",
    "vite": "5.4.8"
  }
}

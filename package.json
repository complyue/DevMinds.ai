{
  "name": "devminds.ai",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev:backend": "pnpm --filter @devminds/backend dev",
    "build:backend": "pnpm --filter @devminds/backend build",
    "dev:web": "pnpm --filter @devminds/webapp dev",
    "build:web": "pnpm --filter @devminds/webapp build",
    "preview:web": "pnpm --filter @devminds/webapp preview",
    "typecheck": "pnpm -r run typecheck",
    "lint": "pnpm -r run lint",
    "format": "pnpm -r --parallel --workspace-concurrency=1 run format",
    "update-models": "node scripts/update-models.js"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "3.6.2",
    "typescript": "5.6.3"
  },
  "lint-staged": {
    "packages/*/src/**/*.{ts,tsx,js,jsx,json,css,scss,md,mdx,html,htm}": "prettier --write --ignore-unknown"
  }
}
